<!-- -->
<launch>
  <!-- Navigation macro-actions -->
  <include file="$(find upo_launchers)/upo_lab/real_upolab_macro_actions.launch"/>

  <!-- People localisation -->
  <include file="$(find uva_launchers)/launch/combined_tracking.launch"/>

  <node pkg="decision_making_uva" name="upo_event_manager" type="upo_event_manager.py" output="screen"/>
  <node pkg="decision_making_uva" name="upo_pilot_interface_bridge" type="upo_pilot_interface_bridge.py" output="screen"/>
  <!--<node pkg="decision_making_uva" name="interaction_target_identifier" type="interaction_target_identifier.py" output="screen"/>-->

  <node pkg="decision_making_uva" name="upo_navigation_behavior_fsm" type="upo_navigation_behavior_fsm.py" output="screen">
    <param name="waypoint_defs" value="$(find upo_launchers)/upo_lab/maps/waypoint_defs.yaml"/>
  </node>

<node pkg="rosbag" type="record" name="record_1" args="/teresa_diagnostics -o /home/teresa/data/teresa_data.bag" />

  <node pkg="smach_viewer" name="smach_viewer" type="smach_viewer.py"/>
</launch>
